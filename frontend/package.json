    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@ngstack/code-editor": "^5.1.0",
    "angular-oauth2-oidc": "^15.0.1",
    "clipboard": "^2.0.11",
    "marked": "^4.3.0",
    "ngx-highlightjs": "^10.0.0",
