{
  "name": "aiod-rail-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-local": "ng serve --watch --configuration local",
    "build-prod": "ng build --configuration production --output-path=dist",
    "build-dev": "ng build --configuration development --output-path=dist",
    "build-local": "ng build --configuration local --output-path=dist",
    "watch": "ng build --watch --configuration development",
